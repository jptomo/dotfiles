[web]
cacerts = /etc/ssl/certs/ca-certificates.crt

[ui]
username = Tomohiro Nakamura <quickness.net@gmail.com>
ignore = ~/.hgignore
style = compact

[merge-tools]
diff3.args = -m $local $base $other > $output

